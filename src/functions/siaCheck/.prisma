datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model SIABadge {
  id Int @id
  name String
  description String
  image String
  created_at DateTime
  updated_at DateTime

  account Account @relation(fields: [account_id], references: [id])
  account_id String
}

model Account {
  id String @id @default(uuid())
  sia SIABadge[]
}