datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Application {
  id String @id @default(uuid())
  name String
  account Account @relation(fields: [accountId], references: [id])
  accountId String

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
  deleted DateTime?
}

model Account {
  id String @id @default(uuid())
  application Application[]
}